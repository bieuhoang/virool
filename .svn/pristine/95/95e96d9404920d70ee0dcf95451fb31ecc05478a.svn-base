#brands-hero-unit{
	background: url(../images/hero-brands.jpg) no-repeat center center !important;
}
.hero-inner{
	background: url(../images/hero-headline.jpg)  no-repeat center center !important;
}
.hero-inner h1{
	font-family: "SteelfishRg-regular";
	font-style: normal;
    font-weight: normal;
    text-transform: uppercase;
    font-size: 80px !important;
}
#advertiser-targeting-tools {
    background: url(../images/musician-hero-tools.jpg) center center #010F27;
    height: 400px;
    padding-top: 60px;
}
.brands-carousel{
	position: relative;
}
.brands-carousel a.left{
	left: -10px;
    position: absolute;
    top: 70px;
}
.brands-carousel a.right{
	right: -30px;
    position: absolute;
    top: 70px;
}

